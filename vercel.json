{
    "version": 2,
    "routes": [
        {
            "src": "/_nuxt/.+",
            "headers": {
                "Cache-Control": "max-age=31557600"
            }
        },
        {
            "src": "/sw.js",
            "dest": "/_nuxt/static/sw.js",
            "headers": {
                "cache-control": "public, max-age=43200, immutable",
                "Service-Worker-Allowed": "/"
            }
        },
        {
			"src": "^\/blog\/[A-Za-z0-9][-]{1,}\/[A-Za-z0-9][-]{1,}($|\/)",
			"dest": "/blog/category/slug"
		},
        {
			"src": "^\/blog\/[A-Za-z0-9][-]{1,}($|\/)",
			"dest": "/blog/category"
		},
        {
			"src": "^\/blog($|\/)",
			"dest": "/blog"
		},
        {
			"src": "^\/art($|\/)",
			"dest": "/art"
		},
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}